[_setup_]
port=COM1
baudrate=115200

decoder = hdlc

display = list
display_sep = ,
display_skip_transparent = 0


width=600
height=201
background_color = white
grid_h_origin = 200
grid_h_step = 100
grid_h_color = #EEE
grid_h_origin_color = #CCC
grid_v_origin = 0
grid_v_step = 10
grid_v_color = #EEE
grid_v_origin_color = transparent


[_default_]
min=-100
max=100
color=transparent
format = %d
type = sbyte

[controlPitch]
color=red

[controlRoll]
color=orange

[controlYaw]
color=green

